Algoritmo consumo_de_combustible
	definir kilometros Como numerico
	definir consumo Como numerico
	definir rendimiento como numerico
	rendimiento <- 20.8
	Escribir "Señale los kilómetros a recorrer:"
	Leer kilometros
	consumo <- kilometros/rendimiento
	Escribir "Conductor, durante su recorrido ha consumido: ", consumo, " litros"

	
	
	
FinAlgoritmo
